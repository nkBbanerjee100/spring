<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<!-- this is our beans -->
	<!-- using setter method
	
	<bean class="com.spingcore.spring.Student" name="student1">
	
	<property name="studentId" >
		<value>22334</value>
	</property>

	<property name="studentAddress">
	<value>
		Delhi
	</value>
	</property>
	</bean>
	
	 -->
	 <!-- 
	 using p-schema
	  -->
	 <!-- this is our beans -->
	<bean class="com.spingcore.spring.Student" name="student1"  p:studentId="24234" p:sname="Ram Singh" p:studentAddress="Delhi"    />

	<!-- second bean  -->
	<bean class="com.spingcore.spring.Student" name="student2"  p:studentId="1341" p:sname="Shyam Singh" p:studentAddress="Lucknow" />
	 
	
</beans>