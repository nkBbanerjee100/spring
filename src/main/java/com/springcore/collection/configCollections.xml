<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
						http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean name="nk"
		class="com.springcore.collection.CollectionsChecking">
		<property name="name" value="nk" />

		<property name="phones">
			<list>
				<value>10</value>
				<value>100</value>
				<value>120</value>
				<value>140</value>
				<null />
			</list>
		</property>

		<property name="addresses">
			<set>
				<value>bankura</value>
				<value>las vegas</value>
				<value>north western ghats</value>
				<value>sundarabns</value>
			</set>
		</property>

		<property name="courses">
			<map>
				<entry key="java" value="2 months" />
				<entry key="react" value="1 month" />
				<entry key="c++" value="3.5 months" />
				<entry key="python" value="1 month" />
			</map>
		</property>
		<property name="props">
			<props>
				<prop key="nameOfStudent">NK Banerjee</prop>
				<prop key="ageOfStudent">23</prop>
			</props>
		</property>

	</bean>
</beans>